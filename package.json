{
  "name": "gpt-assist",
  "displayName": "gpt-assist",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gpt-assist.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "gpt-assist.call_gpt",
        "title": "Call GPT"
      }
    ],
    "keybindings": [
      {
        "command": "gpt-assist.call_gpt",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "dotenv": "^16.0.3",
    "nedb-promises": "^6.2.1",
    "openai": "^3.2.1"
  }
}
